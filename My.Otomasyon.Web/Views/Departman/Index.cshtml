
@model List<DepartmanListView>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<table class="table table-bordered" style="margin-top:20px">
    <tr>

        

        <th>Departman Name</th>
        <th>Delete</th>
        <th>Update</th>
        <th>Details</th>
    </tr>
    @foreach (var item in Model)
    {


        <tr>
           

            <td>
                @item.DepartmanName
            </td>


            <td>
                <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
            </td>
            <td>
                <a asp-action="Update" asp-route-id="@item.Id" class="btn btn-warning">Update</a>
            </td>
            <td>
                <a asp-action="DepartmanDetail" asp-route-id="@item.Id" class="btn btn-default" style="background-color:chocolate ">Details</a>
            </td>


        </tr>

    }
</table>
<a href="/Departman/Add" class="btn btn-primary">New Departmant</a>

