@model List<PerconelListViewModel>
@{
    ViewData["Title"] = "DepartmanDetail";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active">@ViewBag.DepName</li>
</ol>

<table class="table table-hover">
    <tr>

        <th>
           Name

        </th>
        <th>
            Surname

        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>@item.Name</td>
            <td>@item.Surname</td>           
            <td> <a class="btn btn-default" href="/Departman/SellsDetail/@item.Id" style="background-color:lightgreen">Sells</a></td>
        </tr>
    }

</table>

